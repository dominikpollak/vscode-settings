  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.moveEditorToPreviousGroup"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.moveEditorToNextGroup"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["workbench.action.toggleMaximizeEditorGroup"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["vscode-harpoon.addEditor"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["vscode-harpoon.editEditors"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["vscode-harpoon.gotoPreviousHarpoonEditor"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["vscode-harpoon.addEditor1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["vscode-harpoon.addEditor2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["vscode-harpoon.addEditor3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["vscode-harpoon.addEditor4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["vscode-harpoon.addEditor5"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.commentLine"],
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    },
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["d", "0"],
      "after": ["d", "^"]
    },
    {
      "before": ["c", "0"],
      "after": ["c", "^"]
    },
    {
      "before": ["<S-l>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-h>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "x"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "n", "c"],
      "commands": ["merge-conflict.next"]
    },
    {
      "before": ["<leader>", "n", "p"],
      "commands": ["merge-conflict.previous"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["<leader>", "r", "a"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["K"],
      "after": ["g", "h"]
    },
    {
      "before": ["0"],
      "after": ["0", "w"]
    }
  ],
  "vim.insertModeKeyBindings": [
    // {
    //   "before": ["j", "j"],
